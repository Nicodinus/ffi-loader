{
    "name": "nicodinus/php-ffi-loader",
    "homepage": "https://github.com/Nicodinus/php-ffi-loader",
    "description": "FFI libraries loader with simple preprocessor",
    "keywords": [
        "ffi",
        "parser",
        "compiler",
        "loader",
        "preprocessor"
    ],
    "type": "lib",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicodinus",
            "email": "nicodinus@yandex.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "Serafim\\FFILoader\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Serafim\\FFILoader\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=7.4",
        "phplrt/lexer-contracts": "^3.0"
    },
    "require-dev": {
        "larapack/dd": "^1.1",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
